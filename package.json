{
  "name": "shopease_backend",
  "version": "1.0.0",
  "description": "Ecommerce project",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "npx sequelize-cli db:migrate --config sequelize.config.js",
    "db:seed": "npx sequelize-cli db:seed:all --config sequelize.config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all --config sequelize.config.js",
    "db:drop": "npx sequelize-cli db:drop --config sequelize.config.js",
    "db:create": "npx sequelize-cli db:create --config sequelize.config.js",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "author": "Rao Nauman",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
